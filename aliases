e.=explorer .
b=bash --login -i
ls=ls --show-control-chars -F --color $*
clear=cls
unalias=alias /d $1
tc.=%COMMANDER_EXE% /O /T /R="%CD%"
which=where $*
gl=git log --oneline --graph --decorate  $*
s=git status  
gf=git fetch  
gpr=git pull -r
grh=git reset --hard
log=TortoiseGitProc.exe /command:log
_stashPullPush=git stash && git pull -r && git push && git stash pop
_pullPush=git pull -r && git push
_mergeDev=git fetch && git merge origin/develop
